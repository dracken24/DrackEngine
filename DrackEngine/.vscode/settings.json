{
    "workbench.colorTheme": "Aura Dark (Soft Text)",
    "editor.minimap.enabled": true,
    "terminal.integrated.fontWeight": "1",
    "editor.fontFamily": "'Fira Code', 'Consolas', 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.fontWeight": "1",
    "workbench.editor.showTabs": "multiple",
    "workbench.colorCustomizations": {
      "editorCursor.foreground": "#b348d7",
      "editor.lineHighlightBackground": "#6d335a5f",
      "editor.lineHighlightBorder": "#00ff405f"
    },
    "workbench.statusBar.visible": true,
    "editor.lineNumbers": "on",
    "editor.cursorStyle": "line",
    "window.zoomLevel": 1,
    "editor.tabCompletion": "on",
    "todohighlight.isEnable": true,
    "todohighlight.keywords": [
      {
        "text": "TODO:",
        "color": "red",
        "backgroundColor": "rgba(0,0,0,0)"
      },
      {
        "text": "HACK:",
        "color": "#ff6600",
        "backgroundColor": "rgba(0,0,0,0)"
      },
      {
        "text": "NOTE:",
        "color": "#3340FD",
        "backgroundColor": "rgba(0,0,0,0)"
      },
      {
        "text": "WARNING:",
        "color": "yellow",
        "backgroundColor": "rgba(0,0,0,0)"
      },
      {
        "text": "KEEP:",
        "color": "#12b6a5",
        "backgroundColor": "rgba(0,0,0,0)"
      }
    ],
    "todohighlight.defaultStyle": {
      "color": "red",
      "backgroundColor": "#ffab00",
      "overviewRulerColor": "rgba(0,0,0,0)",
      "isWholeLine": false
    },
    "files.exclude":
    {
      "**/.DS_Store":true,
      "**/.git":true,
      "**/.gitmodules":true,
      "**/*.booproj":true,
      "**/*.pidb":true,
      "**/*.suo":true,
      "**/*.user":true,
      "**/*.userprefs":true,
      "**/*.unityproj":true,
      "**/*.dll":true,
      "**/*.exe":true,
      "**/*.pdf":true,
      "**/*.mid":true,
      "**/*.midi":true,
      "**/*.wav":true,
      "**/*.gif":true,
      "**/*.ico":true,
      "**/*.jpg":true,
      "**/*.jpeg":true,
      "**/*.png":true,
      "**/*.psd":true,
      "**/*.tga":true,
      "**/*.tif":true,
      "**/*.tiff":true,
      "**/*.3ds":true,
      "**/*.3DS":true,
      "**/*.fbx":true,
      "**/*.FBX":true,
      "**/*.lxo":true,
      "**/*.LXO":true,
      "**/*.ma":true,
      "**/*.MA":true,
      "**/*.obj":true,
      "**/*.OBJ":true,
      "**/*.asset":true,
      "**/*.cubemap":true,
      "**/*.flare":true,
      "**/*.mat":true,
      "**/*.meta":true,
      "**/*.prefab":true,
      "**/*.unity":true,
      "build/":true,
      "Build/":true,
      "Library/":true,
      "library/":true,
      "obj/":true,
      "Obj/":true,
      "ProjectSettings/":true,
      "temp/":true,
      "Temp/":true
    },
    "dotnet.defaultSolution": "Infinite Discovery.sln"
}